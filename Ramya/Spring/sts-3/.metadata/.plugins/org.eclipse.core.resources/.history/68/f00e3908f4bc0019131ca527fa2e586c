package com.rt;

public class User {
 Calc calc;
 int a,b;
 public int getA() {
	return a;
}
public void setA(int a) {
	this.a = a;
}
public int getB() {
	return b;
}
public void setB(int b) {
	this.b = b;
}
public Calc getCalc() {
	return calc;
}
public void setCalc(Calc calc) {
	this.calc = calc;
}
public void Addition() {
	calc.add();
	
 }
 
 
}
